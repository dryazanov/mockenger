  "description": "Frontend for the mock server",
  "private": true,
  "main": "index.html",
  "scripts": {
    "build:dev": "gulp --environment development --security false",
    "build:prod": "gulp --environment production --security false",
    "build:dev:security": "gulp --environment development --security true",
    "build:prod:security": "gulp --environment production --security true",
    "dev": "gulp --environment development --security false && gulp webServer",
    "prod": "gulp --environment production --security false && gulp webServer",
    "dev:security": "gulp --environment development --security true && gulp webServer",
    "prod:security": "gulp --environment production --security true && gulp webServer",
